{
  "name": "react-flaticons",
  "version": "0.0.1",
  "description": "A React component library that provides easy access to free vector icons from the FlatIcon website.",
  "main": "build/index.js",
  "module": "dist/index.js",
  "sideEffects": false,
  "typings": "dist/index.d.ts",
  "scripts": {
    "postinstall": "rm -rf ./src/assets/ && cp -r node_modules/@flaticon/flaticon-uicons/css ./src/assets/",
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "rm -rf src/icons && node bin/build.js",
    "build:bundle": "rm -rf build && rollup --config rollup.config.js",
    "build:es": "rm -rf dist && babel src --out-dir dist --copy-files",
    "build": "concurrently \"npm:build:*\""
  },
  "files": [
    "dist",
    "es"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hamedyosefian/react-flaticons.git"
  },
  "keywords": [
    "react",
    "react-component",
    "react-icons",
    "uicons",
    "icons",
    "flaticon"
  ],
  "author": "Hamed Yousefian , Mohammad Shariat",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hamedyosefian/react-flaticons/issues"
  },
  "homepage": "https://github.com/hamedyosefian/react-flaticons#readme",
  "dependencies": {
    "@flaticon/flaticon-uicons": "^3.2.1",
    "opentype.js": "^1.3.4"
  }
}
